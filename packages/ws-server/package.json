    "koa-websocket": "^7.0.0",
    "pino": "^7.11.0",
    "pino-pretty": "^7.6.1"
  },
  "devDependencies": {
    "@hashgraph/sdk": "^2.54.0-beta.1",
    "@koa/cors": "^5.0.0",
    "@types/chai": "^4.3.0",
    "@types/cors": "^2.8.12",
